/usr/bin/moc-qt4:
	(cd $(QTDIR)/src/tools/moc && $(MAKE))

mocclean: compiler_moc_header_clean compiler_moc_source_clean

mocables: compiler_moc_header_make_all compiler_moc_source_make_all

compiler_moc_header_make_all: moc_pzgtools.cpp
compiler_moc_header_clean:
	-$(DEL_FILE) moc_pzgtools.cpp
moc_pzgtools.cpp: ui_pzgtools.h \
		pzgtools.h \
		/usr/bin/moc-qt4
	/usr/bin/moc-qt4 $(DEFINES) $(INCPATH) pzgtools.h -o moc_pzgtools.cpp

compiler_rcc_make_all: qrc_pzg.cpp
compiler_rcc_clean:
	-$(DEL_FILE) qrc_pzg.cpp
qrc_pzg.cpp: pzg.qrc \
		/usr/bin/rcc \
		icons/Settings2.png
	/usr/bin/rcc -name pzg pzg.qrc -o qrc_pzg.cpp

compiler_image_collection_make_all: qmake_image_collection.cpp
compiler_image_collection_clean:
	-$(DEL_FILE) qmake_image_collection.cpp
compiler_moc_source_make_all:
compiler_moc_source_clean:
compiler_uic_make_all: ui_pzgtools.h
compiler_uic_clean:
	-$(DEL_FILE) ui_pzgtools.h
ui_pzgtools.h: pzgtools.ui
	/usr/bin/uic-qt4 pzgtools.ui -o ui_pzgtools.h

compiler_clean: compiler_moc_header_clean compiler_rcc_clean compiler_image_collection_clean compiler_moc_source_clean compiler_uic_clean 

