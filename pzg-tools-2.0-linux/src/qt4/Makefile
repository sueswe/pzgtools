dist: 
	@$(CHK_DIR_EXISTS) .tmp/qt41.0.0 || $(MKDIR) .tmp/qt41.0.0 
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/qt41.0.0/ && $(COPY_FILE) --parents pzgtools.h .tmp/qt41.0.0/ && $(COPY_FILE) --parents pzg.qrc .tmp/qt41.0.0/ && $(COPY_FILE) --parents main.cpp pzgtools.cpp .tmp/qt41.0.0/ && $(COPY_FILE) --parents pzgtools.ui .tmp/qt41.0.0/ && (cd `dirname .tmp/qt41.0.0` && $(TAR) qt41.0.0.tar qt41.0.0 && $(COMPRESS) qt41.0.0.tar) && $(MOVE) `dirname .tmp/qt41.0.0`/qt41.0.0.tar.gz . && $(DEL_FILE) -r .tmp/qt41.0.0


